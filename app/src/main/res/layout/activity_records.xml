<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back_3"
    tools:context=".Records_Activity">

    <TextView
        android:id="@+id/txt_profile5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:elevation="10dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/profile"
        android:textColor="@color/dark_blue"
        app:drawableTint="#FF9800"
        app:layout_constraintBottom_toBottomOf="@+id/button4"
        app:layout_constraintEnd_toEndOf="@+id/button4"
        app:layout_constraintHorizontal_bias="0.607"
        app:layout_constraintStart_toStartOf="@+id/button4"
        app:layout_constraintTop_toTopOf="@+id/button4"
        app:layout_constraintVertical_bias="0.928" />

    <TextView
        android:id="@+id/txt_records5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="10dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/records"
        android:textColor="@color/white"
        app:drawableTint="@color/dark_blue"
        app:layout_constraintBottom_toBottomOf="@+id/btn_records5"
        app:layout_constraintEnd_toEndOf="@+id/btn_records5"
        app:layout_constraintHorizontal_bias="0.696"
        app:layout_constraintStart_toStartOf="@+id/btn_records5"
        app:layout_constraintTop_toTopOf="@+id/btn_records5"
        app:layout_constraintVertical_bias="0.59"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/btn_records5"
        android:layout_width="125dp"
        android:layout_height="48dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/btn_bg_yell"
        android:contentDescription="@string/records"
        android:drawableStart="@drawable/ic_baseline_newspaper_24"
        android:drawableTint="@color/white"
        android:fontFamily="@font/poppins_bold"
        android:paddingStart="10dp"
        android:textColor="@color/dark_blue"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <Button
        android:id="@+id/button4"
        android:layout_width="125dp"
        android:layout_height="48dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="60dp"
        android:background="@drawable/box"
        android:contentDescription="@string/profile"
        android:drawableStart="@drawable/ic_baseline_person"
        android:drawableTint="@color/dark_blue"
        android:paddingStart="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <Button
        android:id="@+id/btn_remaining2"
        android:layout_width="137dp"
        android:layout_height="50dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="5dp"
        android:backgroundTint="#FEEEEE"
        android:fontFamily="@font/poppins_bold"
        android:paddingStart="60dp"
        android:paddingTop="25dp"
        android:text="@string/_1_203_09"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_prof" />

    <TextView
        android:id="@+id/txt_rewards2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        android:elevation="10dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/reward_balance"
        android:textColor="@color/dark_blue"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="@+id/btn_remainingbal2"
        app:layout_constraintTop_toTopOf="@+id/btn_remainingbal2" />

    <Button
        android:id="@+id/btn_prof"
        android:layout_width="298dp"
        android:layout_height="101dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="60dp"
        android:backgroundTint="#FEEEEE"
        android:fontFamily="@font/poppins_bold"
        android:paddingEnd="50dp"
        android:paddingBottom="40dp"
        android:text="@string/reiner_gena"
        android:textColor="@color/dark_blue"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_bal2"
        android:layout_width="117dp"
        android:layout_height="21dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        android:elevation="10dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/remaining_balance"
        android:textColor="@color/dark_blue"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="@+id/btn_remaining2"
        app:layout_constraintTop_toTopOf="@+id/btn_remaining2" />

    <TextView
        android:id="@+id/txt_acctype2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:elevation="10dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/student_account"
        android:textColor="@color/dark_blue"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_prof"
        app:layout_constraintEnd_toEndOf="@+id/btn_prof"
        app:layout_constraintHorizontal_bias="0.331"
        app:layout_constraintStart_toStartOf="@+id/btn_prof"
        app:layout_constraintTop_toTopOf="@+id/btn_prof1"
        app:layout_constraintVertical_bias="0.535" />

    <Button
        android:id="@+id/btn_remainingbal2"
        android:layout_width="137dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="60dp"
        android:backgroundTint="#FEEEEE"
        android:fontFamily="@font/poppins_bold"
        android:paddingStart="80dp"
        android:paddingTop="25dp"
        android:text="@string/_25_18"
        android:textColor="#F10C0C"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_prof" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="304dp"
        android:layout_height="368dp"
        android:layout_marginTop="65dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_prof">

        <Button
            android:id="@+id/btn_ticket"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:background="@drawable/box"
            android:fontFamily="@font/poppins_bold"
            android:gravity="center"
            android:paddingStart="10dp"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:shadowColor="#000000"
            android:text="@string/ticket_copy"
            android:textColor="@color/dark_blue" />

        <Button
            android:id="@+id/btn_receipt"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:background="@drawable/box"
            android:fontFamily="@font/poppins_bold"
            android:gravity="center"
            android:paddingStart="10dp"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:shadowColor="#000000"
            android:text="@string/receipt"
            android:textColor="@color/dark_blue" />

        <Button
            android:id="@+id/btn_stationhis"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:background="@drawable/box"
            android:fontFamily="@font/poppins_bold"
            android:gravity="center"
            android:paddingStart="10dp"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:shadowColor="#000000"
            android:text="@string/station_history"
            android:textColor="@color/dark_blue" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>